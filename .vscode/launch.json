  "version": "0.2.0",
  "configurations": [
    // macOS LLDB configurations
    { 
      "name": "(lldb) Launch qemu --help",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:HOME}/Work/qemu-arm-dev/darwin-x64/install/qemu-arm/bin/qemu-system-gnuarmeclipse",
      "args": [
        "--help"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": true,
      "MIMode": "lldb"
    },
    {
      "name": "(lldb) Launch qemu f4b-sh --nographic",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:HOME}/Work/qemu-arm-dev/darwin-x64/install/qemu-arm/bin/qemu-system-gnuarmeclipse",
      "args": [
        "--verbose",
        "--verbose",
        "--board",
        "STM32F4-Discovery",
        "--mcu",
        "STM32F407VG",
        "-d",
        "unimp,guest_errors",
        "--nographic",
        "--image",
        "${env:HOME}/Work/qemu-eclipse-test-projects.git/f407-disc-blink-tutorial/Debug/f407-disc-blink-tutorial.elf",
        "--semihosting-config",
        "enable=on,target=native",
        "--semihosting-cmdline",
        "f4b",
        "20"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": true,
      "MIMode": "lldb"
    },
